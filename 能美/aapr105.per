             rvu04   = rvu04;
             rvu00   = rvu00,COMMENTS=" 1.Sotck-in 2.Reject 3.Inventory Return";
             rvu01   = rvu01;
  DATEEDIT   rvu03   = rvu03;
             rvb22   = rvb22;
  RADIOGROUP n       = FORMONLY.amt_sw ,
                       ITEMS=((1,"Receipt Amount"),(2,"APEntering Account Amount")),
                       DEFAULT="1";
  RADIOGROUP c       = FORMONLY.LC_sw ,
                       ITEMS=((1,"Non L/C Category"),(2,"L/CCa"),(3,"All ")),
                       DEFAULT="3";
  CHECKBOX   m       = FORMONLY.more  , NOT NULL ,
                       VALUECHECKED="Y", VALUEUNCHECKED="N", TEXT="Other codition to print";
END
